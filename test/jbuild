(jbuild_version 1)

(executables
 (
  (names (units vector3))
  (libraries (lgo oUnit))
 )
)

(alias
 (
  (name    runtest)
  (package lgo)
  (deps    (units.exe))
  (action 
	(run ${exe:units.exe})
  )
 )
)

(alias
 (
  (name    runtest)
  (package lgo)
  (deps    (vector3.exe))
  (action 
	(run ${exe:vector3.exe})
  )
 )
)
